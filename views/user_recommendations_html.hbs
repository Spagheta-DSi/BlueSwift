{{#each data}}
{{#if data}}
<div class="js-account-summary account-summary js-actionable-user " data-user-id="{{ data.did }}" data-feedback-token="180" data-impression-id=""  >
  <div class="dismiss js-action-dismiss"><span class="Icon Icon--close"></span></div>
  <div class="content"> <a class="account-group js-recommend-link js-user-profile-link user-thumb" href="/profile/{{ data.handle }}" data-user-id="{{ data.did }}"> <img class="avatar js-action-profile-avatar " src="{{ data.avatar }}" alt=""> <span class="account-group-inner js-action-profile-name" data-user-id="586671909"> <b class="fullname">{{ data.displayName }}</b> <span>&rlm;</span> <span class="visuallyhidden">Verified account</span></span> <span class="username"><s>@</s><span class="js-username">{{ data.handle }}</span></span> </span> </a> <small class="metadata social-context"> </small>
    <div class="user-actions not-following" data-user-id="{{ data.did }}" >
      <button type="button" class="small-follow-btn follow-btn btn small follow-button js-recommended-item">
      <div class="js-action-follow follow-text action-text"> <span class="Icon Icon--follow"></span> Follow </div>
      <div class="js-action-unfollow following-text action-text">Following</div>
      <div class="js-action-unfollow unfollow-text action-text">Unfollow</div>
      <div class="block-text action-text">Blocked</div>
      <div class="js-action-unblock unblock-text action-text">Unblock</div>
      <div class="js-action-unfollow pending-text action-text">Pending</div>
      <div class="js-action-unfollow cancel-req-text action-text">Cancel</div>
      </button>
    </div>
  </div>
</div>
<div class="dismiss js-action-dismiss"><span class="Icon Icon--close"></span></div>
{{/if}}
{{/each}}